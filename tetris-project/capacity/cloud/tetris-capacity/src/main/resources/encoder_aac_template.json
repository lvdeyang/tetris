{
	"aac": {
		"type": "container",
		"items": {
			"type": "enum",
			"constraint": {
				"values":[
					"bitrate",
					"sample_rate",
					"sample_fmt",
					"channel_layout",
					"type"
				]
			}
		},
		"constraint": {
			"bitrate": {
				"type": "enum",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 7,
					"values": [
						"12",
						"32",
						"48",
						"56",
						"64",
						"80",
						"96",
						"128",
						"192",
						"256",
						"272"
					]
				}
			},
			"sample_rate": {
				"type": "enum",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 7,
					"values": [
						"8",
						"11.025",
						"12",
						"16",
						"22.05",
						"24",
						"32",
						"44.1",
						"48",
						"64",
						"88.2",
						"96"
					]
				}
			},
			"sample_fmt": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"s16"
					]
				}
			},
			"channel_layout": {
				"type": "enum",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"mono",
						"stereo",
						"2.1",
						"3.0",
						"4.0",
						"5.0",
						"5.1"
					]
				}
			},
			"type": {
				"type": "enum",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"mpeg4-aac-lc",
						"mpeg4-he-aac-lc",
						"mpeg4-he-aac-v2-lc"
					]
				}
			}
		}
	}
}