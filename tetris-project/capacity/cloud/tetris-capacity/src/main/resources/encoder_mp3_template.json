{
	"mp3": {
		"type": "container",
		"items": {
			"type": "enum",
			"constraint": {
				"values":[
					"bitrate",
					"sample_rate",
					"sample_fmt",
					"channel_layout"
				]
			}
		},
		"constraint": {
			"bitrate": {
				"type": "enum",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 6,
					"values": [
						"32",
						"48",
						"56",
						"64",
						"80",
						"96",
						"128",
						"192",
						"256"
					]
				}
			},
			"sample_rate": {
				"type": "enum",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 7,
					"values": [
						"8",
						"11.025",
						"12",
						"16",
						"22.05",
						"24",
						"32",
						"44.1",
						"48"
					]
				}
			},
			"sample_fmt": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"s16",
						"s16_planner",
						"s32_planner"
					]
				}
			},
			"channel_layout": {
				"type": "enum",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"mono",
						"stereo"
					]
				}
			}
		}
	}
}