{
	"codec_type": {
		"type": "constant",
		"require": "true",
		"constraint": {
			"const_type": "string",
			"const_value": "h264"
		}
	},
	"video_encoder_type": {
		"type": "constant",
		"require": "true",
		"constraint": {
			"const_type": "string",
			"const_value": "x264"
		}
	},
	"profile": {
		"type": "constant",
		"require": "true",
		"constraint": {
			"const_type": "string",
			"const_value": "main"
		}
	},
	"level": {
		"type": "constant",
		"require": "true",
		"constraint": {
			"const_type": "string",
			"const_value": "40"
		}
	},
	"rc_mode": {
		"type": "constant",
		"require": "true",
		"constraint": {
			"const_type": "string",
			"const_value": "cbr"
		}
	},
	"h264":{
		"type": "container",
		"items": {
			"type": "enum",
			"constraint": {
				"values": [
					"width",
					"height",
					"ratio",
					"fps",
					"rc_mode",
					"gop_size",
					"bitrate",
					"max_bitrate",
					"open_gop",
					"profile",
					"level",
					"ref_frames",
					"lookahead",
					"scenecut",
					"mbtree",
					"entropy_type",
					"encoding_type"
				]
			}
		},
		"constraint": {
			"resolution": {
				"type": "rsl",
				"require": true,
				"forbidden":false,
				"constraint": {
					"dynamic_constraint":{            
						"width": {
							"min_value": 32,
							"max_value": 2050,
							"step": 2,
							"default_value":720
						},
						"height":{
							"min_value": 32,
							"max_value": 1100,
							"step": 2,
							"default_value":576
						}
					},
					"enum_constraint": {
						"default_value": 1,
						"values": [
							"640x480",
							"720x576",
							"1920x1080",
							"follow_src"
						]
					}
				}
			},
			"ratio": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 4,
					"values": [
						"1:1",
						"2:1",
						"4:3",
						"5:4",
						"16:9",
						"16:10"
					]
				}
			},
			"fps": {
				"type": "fps",
				"require": true,
				"forbidden":false,
				"constraint": {
					"dynamic_constraint": {  
						"default_value":25,
						"min_value": 1,
						"max_value": 60,
						"step": 1
					},
					"enum_constraint": {
						"values": [
							"25",
							"29.97",
							"30",
							"59.94",
							"60",
							"follow_src"
						]
					}
				}
			},
			"rc_mode": {
				"type": "enum",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 1,
					"values": [
						"cbr",
						"vbr",
						"abr",
						"nearcbr"
					]
				}
			},
			"gop_size": {
				"type": "num",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 25,
					"min_value": 1,
					"max_value": 500,
					"step": 1
				}
			},
			"bitrate": {
				"type": "num",
				"require": true,
				"forbidden":false,
				"constraint": {
					"default_value": 1500000,
					"min_value": 0,
					"max_value": 200000000,
					"step": 1000
				}
			},
			"max_bitrate": {
				"type": "num",
				"require": false,
				"forbidden": true,
				"constraint": {
					"default_value": 1500000,
					"min_value": 0,
					"max_value": 400000000,
					"step": 1000
				}
			},
			"open_gop": {
				"type": "bool",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": false
				}
			},
			"profile": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 1,
					"values": [
						"baseline",
						"main",
						"high"
					]
				}
			},
			"level": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 10,
					"values": [
						"10",
						"11",
						"12",
						"13",
						"20",
						"21",
						"22",
						"30",
						"31",
						"32",
						"40",
						"41",
						"42",
						"50",
						"51"
					]
				}
			},
			"ref_frames": {
				"type": "num",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 2,
					"min_value": 0,
					"max_value": 16,
					"step": 1
				}
			},
			"lookahead": {
				"type": "num",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"min_value": 0,
					"max_value": 100,
					"step": 1
				}
			},
			"scenecut": {
				"type": "bool",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": true
				}
			},
			"mbtree": {
				"type": "bool",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": false
				}
			},
			"entropy_type": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"cabac",
						"cavlc"
					]
				}
			},
			"encoding_type": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"progressive",
						"interlace",
						"mbaff"
					]
				}
			},
			"x264":{
				"type": "container",
				"items": {
					"type": "enum",
					"constraint": {
						"valus":[
							"keyint_min",
							"refine",
							"pixel_format",
							"bframe_reference",
							"bframe_adaptive",
							"me_range",
							"tune_mode"
						]
					}
				},
				"constraint": {
					"keyint_min": {
						"type": "num",
						"require": false,
						"forbidden":false,
						"constraint": {
							"default_value": 0,
							"min_value": 0,
							"max_value": 200,
							"step": 1
						}
					},
					"refine": {
						"type": "num",
						"require": false,
						"forbidden":false,
						"constraint": {
							"default_value": 4,
							"min_value": 1,
							"max_value": 7,
							"step": 1
						}
					},
					"pixel_format": {
						"type": "enum",
						"require": false,
						"forbidden":false,
						"constraint": {
							"default_value": 0,
							"values": [
								"yuv420"
							]
						}
					},
					"bframe_reference": {
						"type": "enum",
						"require": false,
						"forbidden":false,
						"constraint": {
							"default_value": 0,
							"values": [
								"none",
								"strict",
								"normal"
							]
						}
					},
					"bframe_adaptive": {
						"type": "bool",
						"require": false,
						"forbidden":false,
						"constraint": {
							"default_value": true
						}
					},
					"me_range": {
						"type": "num",
						"require": false,
						"forbidden":false,
						"constraint": {
							"default_value": 32,
							"min_value": 16,
							"max_value": 256,
							"step": 1
						}
					},
					"tune_mode": {
						"type": "enum",
						"require": false,
						"forbidden":false,
						"constraint": {
							"default_value": 2,
							"values": [
								"film",
								"animation",
								"lowlatency",
								"source"
							]
						}
					}
				}
			}
		}
	}
}