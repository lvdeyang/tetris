{
	"parrent_template":"encode_h265_template.json",
	"parrent":"hevc",
	"cuda_encode":{
		"type": "container",
		"items": {
			"type": "enum",
			"constraint": {
				"values":[
					"pixel_format",
					"nv_card_idx"
				]
			}
		},
		"constraint": {
			"pixel_format": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"yuv420",
						"yuv422",
						"yuv420_10bit",
						"yuv422_10bit"
					]
				}
			},
			"nv_card_idx": {
				"type": "num",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"min_value": 0,
					"max_value": 4,
					"step": 1
				}
			},
			"tier": {
				"type": "enum",
				"require": false,
				"forbidden":false,
				"constraint": {
					"default_value": 0,
					"values": [
						"main",
						"high"
					]
				}
			}
		}
	}
}