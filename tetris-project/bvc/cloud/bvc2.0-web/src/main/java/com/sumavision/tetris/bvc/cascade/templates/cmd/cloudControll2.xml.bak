	<op>张文军_131</op>	
	<mid>560c467506c341d780474fff1a1741c7</mid>	
	<content>
		#<control>为了避免解析模板解析错误，采用加密传输
		<control>
			<commandname>ptzctrl</commandname>
			<operation>start</operation>
			<seq>65d9b8f9aeaa4d72af2f187c56750b75</seq>
			<ts>1605666719442</ts>
			<devname></devname>
			<devid></devid>
			<tiltservo>
				<direction>d</direction>
				<speed>8</speed>
			</tiltservo>
