<notify>
	<commandname>bizcmd</commandname>
	<operation>maddinc</operation>
	<seq>${seq}</seq>
	<ts>${ts}</ts>
	<gid>${gid}</gid>
	<op>${op}</op>
	<mlist>
		<#if mAddList??>
			<#list mAddList as m>
				<minfo>
					<mid>${m.mid}</mid>
					<mname>${m.mname}</mname>
					<mtype>${m.mtype}</mtype>
					<#if m.pid??>
						<pid>${m.pid}</pid>
					</#if>
				</minfo>
			</#list>
		</#if>
	</mlist>
</notify>
