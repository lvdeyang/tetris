<notify>
	<commandname>bizcnf</commandname>
	<operation>maddinc</operation>
	<seq>${seq}</seq>
	<ts>${ts}</ts>
	<gid>${gid}</gid>
	<op>${op}</op>
	<mlist>
		<#if mAddList??>
			<#list mAddList as m>
				<minfo>
					<mid>${mid}</mid>
					<mname>${mname}</mname>
					<mtype>${mtype}</mtype>
					<#if m.pid??>
						<pid>${m.pid}</pid>
					</#if>
				</minfo>
			</#list>
		</#if>
	</mlist>
</notify>