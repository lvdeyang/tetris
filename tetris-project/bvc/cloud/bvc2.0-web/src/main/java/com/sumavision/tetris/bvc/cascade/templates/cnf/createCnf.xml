<control>
	<commandname>group</commandname>
	<operation>create</operation>
	<seq>${seq}</seq>
	<ts>${ts}</ts>
	<gid>${gid}</gid>
	<subject>${subject}</subject>
	<bizinfo>
		<biztype>cnf</biztype>
		<bizname>${biztype}</bizname>
		<creatorid>${creatorid}</creatorid>
		<topid>${topid}</topid>
		<mlist>
			<#if mlist??>
				<#list mlist as m>
					<minfo>
						<mid>${m.mid}</mid>
						<mname>${m.mname}</mname>
						<mtype>${m.mtype}</mtype>
						<#if m.pid??>
							<pid>${m.pid}</pid>
						</#if>
					</minfo>
				</#list>	
			</#if>
		</mlist>
	</bizinfo>
</control> 